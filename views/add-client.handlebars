            <label class="active" for="Age">Age</label>
        </div>
    </div>

    <div class="row">
        <div class="input-field col s6">
            <input name='Gender' id="Gender" type="text" class="validate">
            <label class="active" for="Gender">Gender</label>
        </div>
    </div>

    <div class="row">
        <div class="input-field col s6">
            <input name='Weight' id="Weight" type="text" class="validate">
